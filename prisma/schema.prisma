// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model employees {
  id          Int      @id @default(autoincrement())
  name        String
  linkedinlink String
  cvfile      String
  profileimage String
  inforoadmap String
  idposition  Int
  email       String  @unique
  password    String
}